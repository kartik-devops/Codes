#include<bits/stdc++.h>
using namespace std;
#define ll long long
    

    
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    
    int l,b,n,L,B;
    cin>>l>>b>>n;
    ll ans=0;
    while(n--){
        cin>>L>>B;
        ans += max((L/l)*(B/b),(B/l)*(L/b));
    }
    cout<<ans<<endl;
    
	return 0;
}
